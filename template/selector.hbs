{{#each character}}
        <div class="col s12 m7 left">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">{{name}}</span>
              <p>{{quote}}</p>
            </div>
            <div class="card-action">
              <button class="selector" id="{{id}}">More information</button>
            </div>
          </div>
        </div>
{{/each}}